{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for Ohmframe Copilot",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:default",
    "dialog:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://ai.ohmframe.com/**" },
        { "url": "https://*.ohmframe.com/**" }
      ]
    }
  ]
}
